// Pan and tilt the antenna to specified degrees.

Command pan_tilt_move_joints (Real pan_degrees, Real tilt_degrees);

PanTiltMoveJoints:
{
  In Real PanDegrees, TiltDegrees;

  SynchronousCommand pan_tilt_move_joints (PanDegrees, TiltDegrees);
}
